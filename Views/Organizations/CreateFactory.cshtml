@model QueueControlServer.Models.Factory
@{
    ViewData["Title"] = "CreateFactory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4></h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateFactory">
            <div class="form-group">
                <label asp-for="OrganizationName" class="control-label"> </label>
                <input asp-for="OrganizationName" class="keyboardInput" maxlength="50" id="output" onblur="jFunction()" />
                <span asp-validation-for="OrganizationName" class="text-danger"> </span>
            </div>
            <div class="form-group">
                <label asp-for="OrganizationId" class="control-label"> </label>
                <input asp-for="OrganizationId" class="keyboardInput" maxlength="50" id="output" onblur="jFunction()" />
                <span asp-validation-for="OrganizationId" class="text-danger"> </span>
            </div>
            <div class="form-group">
                <input type="submit" value="Factory" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Отмена регистрации</a>
</div>
